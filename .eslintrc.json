{
    "env": {
        "browser": true
    },
    "extends": ["airbnb", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {
        "import/core-modules": ["react", "prop-types"]
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
        "react/jsx-indent": [1, 4],
        "react/jsx-indent-props": [1, 4]
    }
}
